   "source": [
    "import matplotlib.pyplot as plt\n",
    "import pandas as pd \n",
    "\n",
    "# CSV読み込み\n",
    "df = pd.read_csv(\"kion10y.csv\", encoding = \"utf-8\")\n",
    "# 月ごとに平均を求める\n",
    "# gropuby(グループ化する列)[計算対象の列]\n",
    "g = df.groupby(['月'])['気温']\n",
    "gg = g.sum() /g.count()\n",
    "\n",
    "# 結果出力\n",
    "print(gg)\n",
    "gg.plot()\n",
    "plt.savefig(\"tenki-heikin-tuki.png\")\n",
    "plt.show()"
   ]
